<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>UtilPack.NuGet.MSBuild</id>
    <version>1.0.0-RC4</version>
    <title>MSBuild Task Factory for NuGet-Oriented MSBuild Tasks</title>
    <authors>Stanislav Muhametsin</authors>
    <owners>Stanislav Muhametsin</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://www.apache.org/licenses/LICENSE-2.0</licenseUrl>
    <projectUrl>https://github.com/CometaSolutions/UtilPack</projectUrl>
    <description>Provides easy way to execute NuGet-based MSBuild tasks.</description>
    <releaseNotes>The task factory learned how to use NuGet RestoreCommand so that separate .References.csproj file is no longer required. Also there is a breaking change is NuGet package loading callback signature - it accepts required package ID, optional package version, and optional assembly path within package, returning Task of Assembly.</releaseNotes>
    <copyright>Stanislav Muhametsin</copyright>
    <tags>utility nuget extensions msbuild task package resolve</tags>
    <repository url="https://github.com/CometaSolutions/UtilPack" />
    <developmentDependency>true</developmentDependency>
    <dependencies/>
  </metadata>
  <files>
    <file src="bin\Release\net45\*.dll" exclude="bin\Release\net45\Microsoft.Build.Framework.dll" target="build\net45\" />
    <file src="bin\Release\netcoreapp1.1\Newtonsoft.Json.dll" target="build\netcoreapp1.1\" />
    <file src="bin\Release\netcoreapp1.1\NuGet*.dll" target="build\netcoreapp1.1\" />
    <file src="bin\Release\netcoreapp1.1\System.ValueTuple.dll" target="build\netcoreapp1.1\" />
    <file src="bin\Release\netcoreapp1.1\UtilPack*.dll" target="build\netcoreapp1.1\" />
    <file src="build\*.*" target="build\" />
  </files>
</package>