  <ItemGroup>
    <ProjectReference Include="..\UtilPack.NuGet.Deployment\UtilPack.NuGet.Deployment.csproj" />
    <ProjectReference Include="..\UtilPack.ProcessMonitor\UtilPack.ProcessMonitor.csproj" />
  </ItemGroup>

</Project>